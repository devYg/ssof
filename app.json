{
  "expo": {
    "name": "ssof",
    "slug": "EXPO_SSOF",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "androidStatusBarColor": "#FF0214",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#FF0214"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ssof.everyn"
    }
    
  }
}